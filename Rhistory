}
dAcc = data.frame(RtMyData,Pconf,Name,AV,AAV,PE,Resp1,logRt,PRt,Repres,Acc)
l <- glmer(formula = Acc ~ AAV + Pconf + Repres + (1 + AAV |Name), family = "binomial", glmerControl(optimizer = "bobyqa"),data=dAcc)
summary(l)
l <- glmer(formula = Acc ~ AAV + Pconf + Repres + (1 + AAV + Pconf |Name), family = "binomial", glmerControl(optimizer = "bobyqa"),data=dAcc)
summary(l)
